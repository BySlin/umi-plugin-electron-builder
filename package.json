{
  "name": "umi-plugin-electron-builder",
  "version": "1.0.2",
  "description": "Umi plugin for electron-builder",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "electron:dev": "cross-env APP_ROOT=src/fixtures/normal umi dev electron",
    "electron:build:win": "cross-env APP_ROOT=src/fixtures/normal umi build electron --win",
    "electron:build:mac": "cross-env APP_ROOT=src/fixtures/normal umi build electron --mac",
    "electron:build:linux": "cross-env APP_ROOT=src/fixtures/normal umi build electron --linux",
    "build": "father-build",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "files": [
    "lib",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BySlin/umi-plugin-electron-builder"
  },
  "keywords": [
    "umi"
  ],
  "authors": [
    "BySlin <by2292735470@gmail.com>"
  ],
  "license": "MIT",
  "bugs": "https://github.com/BySlin/umi-plugin-electron-builder/issues",
  "homepage": "https://github.com/BySlin/umi-plugin-electron-builder/tree/master#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/node": "^12.20.1",
    "async-exit-hook": "^2.0.1",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "cross-env": "^7.0.3",
    "father-build": "^1.19.1",
    "umi": "^3.3.7"
  }
}