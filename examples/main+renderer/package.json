{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "electron:dev": "cross-env APP_ROOT=src/renderer umi dev electron",
    "electron:build:win": "cross-env APP_ROOT=src/renderer umi build electron --win",
    "electron:build:mac": "cross-env APP_ROOT=src/renderer umi build electron --mac",
    "electron:build:linux": "cross-env APP_ROOT=src/renderer umi build electron --linux",
    "rebuild-deps": "electron-builder install-app-deps"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "umi": "^3.3.7"
  },
  "devDependencies": {
    "@types/node": "^12.0.12",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/test": "^3.3.7",
    "cross-env": "^7.0.3",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "name": "electron_builder_app",
  "version": "0.0.1"
}
